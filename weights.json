{
    "C_UGI":
    {
        "name": "iPhone - Champion",
        "ruleSet":
        [
            {
                "id": 1,
                "type": "dataLayer",
                "operator": "device.navPlatform",
                "condition": "contains",
                "value": "iPhone",
                "chain_condition": "or",
                "chain":
                {
                    "type": "dataLayer",
                    "operator": "device.navPlatform",
                    "condition": "contains",
                    "value": "iPod",
                    "chain_condition": "or",
                    "chain":
                    {
                        "type": "dataLayer",
                        "operator": "device.navAgent",
                        "condition": "contains",
                        "value": "iPhone"
                    }
                }
            },
            {
                "id": 2,
                "type": "function",
                "operator": "try{ const sr = window.top.beagleInfoLayer.sr; const hashPct = window.top.beagleInfoLayer.onHashPct; const labThreshold = sr + (100-sr)*0.2; return hashPct >= labThreshold} catch(e){return false;}",
                "condition": "equal",
                "value": true
            }
        ],
        "43":
        {
            "name": "M - C - Price Scarcity",
            "weight": 80,
            "variants":
            {
                "A": 100,
                "B": 100
            }
        }
    },

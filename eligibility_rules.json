            {
                "name": "AddressEntry",
                "type": "element",
                "operator": "style",
                "condition": "contains",
                "selector": ".popup.frame-box",
                "value": "display: block",
                "chain_condition": "or",
                "chain":
                {
                    "name": "AddressEntry",
                    "type": "element",
                    "operator": "style",
                    "condition": "contains",
                    "selector": ".edit-popup.frame-box",
                    "value": "display: block"
